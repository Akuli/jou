import "stdlib/assert.jou"
import "stdlib/ascii.jou"
import "stdlib/io.jou"


def main() -> int:
    f = fopen("sampleinput.txt", "r")
    assert f != NULL

    result = 0

    buf: byte[512]
    while fgets(buf, sizeof(buf) as int, f) != NULL:
        trim_ascii_whitespace(buf)
        best = 0
        for p1 = &buf[0]; *p1 != '\0'; p1++:
            for p2 = &p1[1]; *p2 != '\0'; p2++:
                value = 10*(*p1 - '0') + (*p2 - '0')
                if value > best:
                    best = value
        result += best

    printf("%d\n", result)  # Output: 357

    fclose(f)
    return 0
