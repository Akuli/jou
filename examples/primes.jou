def mod(value: int, modulus: int) -> int:
    return value - (value / modulus) * modulus

def is_prime(n: int) -> bool:
    if n < 2:
        return False

    divisor = 2
    while divisor < n:
        if mod(n, divisor) == 0:
            return False
        divisor = divisor+1
    return True

def main() -> int:
    # Output: 2
    # Output: 3
    # Output: 5
    # Output: 7
    n = 0
    while n < 10:
        if is_prime(n):
            printf("%d\n", n)
        n = n+1
    return 0
