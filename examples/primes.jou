def mod(value: int, modulus: int) -> int:
    return value - (value / modulus) * modulus

def is_prime(n: int) -> bool:
    if n < 2:
        return False

    for divisor = 2; divisor < n; divisor = divisor+1:
        if mod(n, divisor) == 0:
            return False
    return True

def main() -> int:
    # Output: 2
    # Output: 3
    # Output: 5
    # Output: 7
    for n = 0; n < 10; n = n+1:
        if is_prime(n):
            printf("%d\n", n)
    return 0
