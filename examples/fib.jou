cdecl printf(format: byte*, ...) -> int

def fib(n: int) -> int:
    # TODO: no '<=', 'or' operators yet
    if n == 0:
        return 0
    if n == 1:
        return 1
    return fib(n-1) + fib(n-2)

def main() -> int:
    printf("fib(10) = %d\n", fib(10))  # Output: fib(10) = 55
    printf("fib(20) = %d\n", fib(20))  # Output: fib(20) = 6765
    printf("fib(30) = %d\n", fib(30))  # Output: fib(30) = 832040
    return 0
