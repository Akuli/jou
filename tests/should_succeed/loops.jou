import "stdlib/io.jou"


def main() -> int:
    # Output: while 1
    # Output: while 2
    # Output: while 3
    i = 0
    while i < 3:
        printf("while %d\n", ++i)

    # Output: while 1
    # Output: while 3
    i = 0
    while True:
        if i == 1:
            i++
            continue
        printf("while %d\n", ++i)
        if i == 3:
            break

    # Output: for 1
    # Output: for 2
    # Output: for 3
    for i = 1; i <= 3; i++:
        printf("for %d\n", i)

    # Output: for 1
    # Output: for 3
    for i = 1; i <= 3; i++:
        if i == 2:
            continue
        printf("for %d\n", i)

    # Output: forever 1
    # Output: forever 3
    i = 1
    for ;; i++:
        if i == 2:
            continue
        if i == 4:
            break
        printf("forever %d\n", i)

    # Output: forever 1
    # Output: forever 2
    # Output: forever 3
    i = 1
    for ;;:
        printf("forever %d\n", i++)
        if i == 4:
            break

    # Output:    # #### #  #
    # Output:    # #  # #  #
    # Output:    # #  # #  #
    # Output: #### #### ####
    for y = 0; y < 4; y++:
        for x = 0; x < 14; x++:
            if x % 5 != 4 and (y == 3 or (y == 0 and (x == 6 or x == 7)) or (x > 0 and x % 5 == 0) or x % 5 == 3):
                putchar('#')
            else:
                putchar(' ')
        putchar('\n')

    return 0
