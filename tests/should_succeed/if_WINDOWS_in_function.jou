import "stdlib/io.jou"

if not WINDOWS:
    # Low level function for writing to file. Does not exist on Windows.
    declare write(fd: int, buf: byte*, count: int) -> long

def main() -> int:
    # Output: hello
    if WINDOWS:
        printf("hello\n")
    else:
        # If this code is compiled on Windows, it will not link.
        write(1, "hello\n", 6)

    return 0
