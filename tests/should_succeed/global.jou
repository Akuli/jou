import "stdlib/io.jou"

global x: int
global y: int = 100

def increment_both() -> None:
    x++
    y++

def main() -> int:
    printf("%d %d\n", x, y)  # Output: 0 100
    increment_both()
    printf("%d %d\n", x, y)  # Output: 1 101
    increment_both()
    printf("%d %d\n", x, y)  # Output: 2 102

    return 0
