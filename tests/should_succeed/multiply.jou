# TODO: use printf() for displaying results when available
cdecl putchar(ch: int) -> int

def side_effect(ch: int) -> int:
    putchar(ch)
    return ch

def main() -> int:
    # Output: bd
    putchar(7*7*2)  # 98 = ASCII 'b'
    putchar(2*'2')  # 50 = ASCII '2', 100 = ASCII 'd'
    putchar(2*5)    # 20 = ASCII '\n'

    # (ASCII 'a' * ASCII 'e') % 256 = (97*101) % 256 = 9797 % 256 = 69 = ASCII 'E'
    # TODO: this seems dumb, what would C do here?
    # Output: E
    putchar('a'*'e')
    putchar('\n')

    # Output: abc
    x = side_effect('a')*side_effect('b')*side_effect('c')*side_effect('\n')
    return 0
