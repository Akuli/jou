import "stdlib/io.jou"
import "stdlib/limits.jou"


def main() -> int:
    # Output: -128 to 127
    # Output: -32768 to 32767
    # Output: -2147483648 to 2147483647
    # Output: -9223372036854775808 to 9223372036854775807
    printf("%d to %d\n", INT8_MIN, INT8_MAX)
    printf("%d to %d\n", INT16_MIN, INT16_MAX)
    printf("%d to %d\n", INT32_MIN, INT32_MAX)
    printf("%lld to %lld\n", INT64_MIN, INT64_MAX)

    # Output: 255
    # Output: 65535
    # Output: 4294967295
    # Output: 18446744073709551615
    printf("%d\n", UINT8_MAX)
    printf("%d\n", UINT16_MAX)
    printf("%u\n", UINT32_MAX)
    printf("%llu\n", UINT64_MAX)

    return 0
