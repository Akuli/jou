declare putchar(ch: int) -> int

def putchar_pointer(ch: int*) -> void:
    putchar(*ch)
    putchar(*&*ch)

def putchar2(ch: int) -> void:
    putchar_pointer(&ch)

def myputs1(s: byte*) -> void:
    for i = 0; s[i] != '\0'; i++:
        putchar(s[i])

def main() -> int:
    # Output: hhii
    putchar2('h')
    putchar2('i')
    putchar('\n')

    # https://github.com/Akuli/jou/issues/52
    putchar("hey"[1+1])  # Output: y
    putchar('\n')

    myputs1("asd\n")  # Output: asd

    return 0
