import "stdlib/io.jou"
import "stdlib/mem.jou"

const N: int = 3


def main() -> int:
    numbers: int[N]
    printf("%d\n", array_count(numbers))  # Output: 3

    memset(numbers, 0, sizeof(numbers))
    numbers[2]++
    printf("%d %d %d\n", numbers[0], numbers[1], numbers[2])  # Output: 0 0 1

    return 0
