from "stdlib/math.jou" import abs, llabs, fabs, ceil, floor, round, acos, asin, atan, atan2, cos, sin, tan, fmod, sqrt, cbrt, log, log10, log2, exp, exp2, pow, pow10, ldexp, fmax, fmin, hypot
from "stdlib/io.jou" import printf

def main() -> int:
    printf("%d\n", abs(-1))
    # printf("%ll\n", llabs(-114514191918)) issues #216
    printf("%f\n", fabs(-114.514))
    printf("%f\n", ceil(3.14159265358979))
    printf("%f\n", floor(3.14159265358979))
    printf("%f\n", round(3.14159265358979))
    printf("%f\n", acos(-1))
    printf("%f\n", asin(-1))
    printf("%f\n", atan(-1))
    printf("%f\n", atan2(-1, -1))
    printf("%f\n", cos(45))
    printf("%f\n", sin(45))
    printf("%f\n", tan(90))
    printf("%f\n", fmod(9.3, 3.1))
    printf("%f\n", sqrt(5))
    printf("%f\n", cbrt(27))
    printf("%f\n", log(114))
    printf("%f\n", log10(114))
    printf("%f\n", log2(114))
    printf("%f\n", exp(3))
    printf("%f\n", exp2(3))
    printf("%f\n", pow(2, 2))
    #printf("%f\n", pow10(114))
    printf("%f\n", ldexp(10, 2))
    printf("%f\n", fmax(1.14, 5.14))
    printf("%f\n", fmin(1.14, 5.14))
    printf("%f\n", hypot(3, 4))
    return 0
