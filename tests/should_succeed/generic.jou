import "stdlib/io.jou"

class List[T1, T2]:
    x: T1
    y: T2

def main() -> int:
    pair1: List[int, byte*]
    pair1.x = 1
    pair1.y = "hi"
    pair2 = List[int, byte*]{x = 222, y = "hello"}

    printf("%d %s\n", pair1.x, pair1.y)  # Output: 1 hi
    printf("%d %s\n", pair2.x, pair2.y)  # Output: 222 hello

    return 0
