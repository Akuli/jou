import "stdlib/io.jou"

def main()-> int:
    # binary
    printf("%d\n", 0b11111100111)  # Output: 2023
    printf("%lld\n", 0b110110110100110110100101111011001101100111000111011101010 as int64)  # Output: 123456789876543210

    # octal
    printf("%d\n", 0o777) # Output: 511
    printf("%lld\n", 0o6664664573154707352 as int64) # Output: 123456789876543210

    # hex
    printf("%d\n", 0xCafe012)  # Output: 212852754
    printf("%lld\n", 0x1b69b4bd9b38eea as int64)  # Output: 123456789876543210

    return 0
