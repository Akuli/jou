cdecl putchar(ch: int) -> int

def foo() -> void:
    putchar('h')
    putchar('i')
    putchar('\n')

# The skip argument is needed to prevent compile error about dead code.
def should_do_nothing(skip: bool) -> void:
    if skip:
        return
    putchar('x')

def main() -> int:
    should_do_nothing(True)
    foo()  # Output: hi
    return 0
