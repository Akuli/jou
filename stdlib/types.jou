# Check if character is alphanumeric
declare isalnum(c: int) -> int
# Check if character is alphabetic
declare isalpha(c: int) -> int
# Check if character is a control character
declare iscntrl(c: int) -> int
# Check if character is decimal digit
declare isdigit(c: int) -> int
# Check if character has graphical representation
declare isgraph(c: int) -> int
# Check if character is lowercase letter
declare islower(c: int) -> int
# Check if character is printable 
declare isprint(c: int) -> int
# Check if character is a punctuation character
declare ispunct(c: int) -> int
# Check if character is a white-space
declare isspace(c: int) -> int
# Check if character is uppercase letter
declare isupper(c: int) -> int
# Check if character is hexadecimal digit
declare isxdigit(c: int) -> int
# Convert uppercase letter to lowercase
declare tolower(c: int) -> int
# Convert lowercase letter to uppercase
declare toupper(c: int) -> int
